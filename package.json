  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "build-src": "babel src --out-dir tsrc --copy-files",
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-local": "concurrently --kill-others \"npm run start-local-prod\"",
    "start-prod": "better-npm-run start-prod",
    "start-local-prod": "better-npm-run start-local-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build && npm run styleguide && npm run build-src",
    "lint": "eslint -c .eslintrc.js src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start",
    "test-node": "node ./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "node ./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch",
    "styleguide": "node ./node_modules/react-styleguidist/bin/styleguidist build --config webpack/styleguide.config.js",
    "styleguide-watch": "node ./node_modules/react-styleguidist/bin/styleguidist server --config webpack/styleguide.config.js"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./tsrc",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-local-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./tsrc",
        "NODE_ENV": "production",
        "PORT": 3000,
        "APIPORT": 3030,
        "NPM_CONFIG_PRODUCTION": false
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
